if (arrDomain[1] !== 'se') {
arrDomain.shift();
}
strDomain = arrDomain.join('.');
strDomain = '.' + strDomain;
if (typeof isFirst != 'undefined' && isFirst == true) {
document.cookie = 'optListExp=' + encodeURIComponent(d.toUTCString()) + ';domain=' + strDomain + ';path=/;expires=' + d.toUTCString();

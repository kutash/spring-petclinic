} else if (d == -1) {
document.cookie = cookiename + '=' + encodeURIComponent(value) + ';domain=' + strDomain + ';path=/;expires=' + d.toUTCString();
} else {
var getExpiry = getCookieValue('optListExp');
if (getExpiry == '') {
document.cookie = 'optListExp=' + encodeURIComponent(d.toUTCString()) + ';domain=' + strDomain + ';path=/;expires=' + d.toUTCString();
